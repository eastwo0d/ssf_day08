<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta http-equiv="X-UA-Compatible" content="ie=edge">
        <title>Document</title>
    </head>
    <body>
        <h1>Employee list</h1>
        <table border = "1">
            <thead>
                <th>Employee no</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Gender</th>
            </thead>
            <tbody>

                {{#each employee}}
                    <tr>
                        <td>{{this.emp_no}}</td>
                        <td>{{this.first_name}}</td>
                        <td>{{this.last_name}}</td>
                        <td>{{this.gender}}</td>
                    </tr>
                {{/each}}

            </tbody>
        </table>
        <div></div>
        <form action="">
            <input type="hidden", value="{{prev_offset}}" name="offset">
            <button type="submit">Prev</button>
        </form>
        <form action="">
            <input type="hidden", value="{{next_offset}}" name="offset">
            <button type="submit">Next</button>
        </form>
    </body>
</html>